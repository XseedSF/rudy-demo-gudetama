{
  "name": "xs-event-countdown",
  "version": "1.0.0",
  "engines": {
    "node": "8.9.4"
  },
  "description": "Simple project with a node backend, react with rudys front",
  "main": "index.js",
  "scripts": {
    "start": "node index.js && npm open",
    "debug": "set NODE_ENV=development && node --inspect server/index.js && yarn run open",
    "open": "concurrently \"http-server -a localhost -p 3001\" \"open http://localhost:3001/\"",
    "build": "webpack --env=production --progress --profile --colors",
    "test": "jest --watch",
    "coverage": "jest --coverage"
  },
  "author": "Federico Sobral <fsobral@xseed.com.uy>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "express-promise-router": "^3.0.1",
    "express-session": "^1.15.6",
    "mongoose": "^5.0.6",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-first-router": "^0.0.4-rudy",
    "redux-first-router-link": "^0.0.3-rudy",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
